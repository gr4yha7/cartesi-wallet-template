{
  "name": "create-cartesi-wallet-template",
  "version": "0.2.5",
  "description": "A CLI app for setting up a simple Cartesi wallet interaction application (frontend + backend) using Javascript, Typescript and Rust language templates",
  "keywords": [
    "cartesi",
    "wallet",
    "cartesi-wallet",
    "cartesi-template",
    "javascript template",
    "typescript template",
    "rust template"
  ],
  "homepage": "https://github.com/gr4yha7/cartesi-hackathon",
  "bugs": "https://github.com/gr4yha7/cartesi-hackathon/issues",
  "repository": "https://github.com/gr4yha7/cartesi-hackathon",
  "license": "MIT",
  "author": "gr4yha7",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "create-cartesi-wallet-template": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "create-cartesi-wallet-template",
    "commands": "./dist/commands",
    "dirname": "cli",
    "plugins": [],
    "topicSeparator": " ",
    "topics": {}
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "ansi-colors": "^4",
    "giget": "^1.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.5.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
